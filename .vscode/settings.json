{
  "typescript.tsdk": "node_modules/typescript/lib",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#15AB94",
    "activityBar.background": "#15AB94",
    "activityBar.foreground": "#000000",
    "activityBar.inactiveForeground": "#00000099",
    "activityBarBadge.background": "#000000",
    "activityBarBadge.foreground": "#ffffff",
    "commandCenter.border": "#000000",
    "sash.hoverBorder": "#15AB94",
    "statusBar.background": "#15AB94",
    "statusBar.foreground": "#000000",
    "statusBarItem.hoverBackground": "#f04246",
    "statusBarItem.remoteBackground": "#15AB94",
    "statusBarItem.remoteForeground": "#000000",
    "titleBar.activeBackground": "#15AB94",
    "titleBar.activeForeground": "#000000",
    "titleBar.inactiveBackground": "#15AB94",
    "titleBar.inactiveForeground": "#000000",
    "activityBar.activeBorder": "#000000"

  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "todo-tree.general.tags": ["TODO", "REPLACE"],
  "todo-tree.tree.autoRefresh": true,
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "foreground": "#000000",
      "background": "#FFFF00",
      "iconColour": "#000000"
    },
    "REPLACE": {
      "foreground": "#000000",
      "background": "#648843",
      "iconColour": "#000000",
      "icon": "pencil"
    }
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  }
}
